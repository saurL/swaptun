{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "swaptun",
  "version": "0.1.0",
  "identifier": "com.swaptun.app",
  "build": {
    "beforeDevCommand": "pnpm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "swaptun",
        "width": 800,
        "height": 600
      }
    ],
    "security": { "devCsp": "", "csp": null }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {    "deep-link": {
    "mobile": [
      { "host": "swaptun.com" ,"pathPrefix": ["/open"]}
    ]
  }}
}
